{

    "manifest_version": 2,
    "name": "LetterBoxADV",
    "version": "1.0",
  
    "description": "Patch for the letterboxing bypass vulnerability",

    "icons": {
      "48": "icons/border-48.png",
      "96": "icons/border-96.png"
    },
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["main.js"]
      }
    ]
  
  }
